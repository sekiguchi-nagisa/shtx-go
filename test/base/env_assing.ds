# RUN: eval $cmd $self

source ../../shtx
source ../common/assert.ds

## no env
assert !checkenv ZZZZ
assert "$({ fake_eval "printenv ZZZZ"; assert $? == 1; assert !checkenv ZZZZ; })" == ''
assert_fake_eval "HOGE=@@@ printenv HOGE" "@@@"

## assign env
assert !checkenv ZZZZ
assert "$({ fake_eval "
    ZZZZ='echo 'hell \
    printenv \
    ZZZZ"; assert $? == 0; assert !checkenv ZZZZ; })" == $'echo hell'

true

# CHECK_RE: ^$
# CHECKERR_RE: ^$
# STATUS: 0