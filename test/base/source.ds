# RUN: call $cmd $self

source ../../shtx
source ../common/assert.ds

cd $SCRIPT_DIR

assert_fake_eval 'source ./scripts/arg_in_source.sh' \
    'source=bash, args=aaa bbb ccc, len=4' aaa bbb ccc ddd

