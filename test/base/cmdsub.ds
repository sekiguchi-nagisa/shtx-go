# RUN: call $cmd $self

source ../../shtx
source ../common/assert.ds

# for quoted command substitution

## backquote
assert_fake_eval \
    'echo "`echo ABC`"' "ABC"
assert $? == 0

# comment
assert_fake_eval \
    'echo `# echo ABC` DEF' "DEF"
assert $? == 0

## $( )
assert_fake_eval \
    'echo 34"$(echo A B C)"' "34A B C"
assert $? == 0

## muti command
assert_fake_eval \
    'echo "$(echo AAA; echo BBB; echo CCC)"' $'AAA\nBBB\nCCC'
assert $? == 0

